{
  "kind": "collectionType",
  "collectionName": "organizations",
  "info": {
    "name": "Organization",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true,
      "minLength": 2
    },
    "slogan": {
      "type": "string",
      "unique": true
    },
    "logo": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "link": {
      "type": "string",
      "unique": true,
      "minLength": 10
    },
    "summary": {
      "type": "text",
      "required": true,
      "unique": true,
      "minLength": 20
    },
    "video": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "message_link": {
      "type": "string",
      "unique": true,
      "minLength": 10
    },
    "location": {
      "type": "component",
      "repeatable": false,
      "component": "location.address",
      "required": true
    },
    "start_date": {
      "type": "date",
      "required": true
    },
    "end_date": {
      "type": "date"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "community",
        "enterprise",
        "government"
      ]
    }
  }
}
